{
  "name": "react-native-iconsax",
  "version": "1.0.5",
  "description": "Iconsax icons as React Native component",
  "main": "lib/index.js",
  "typings": "lib/react-native-iconsax.d.ts",
  "scripts": {
    "generate-icons": "svgr -d src assets/linear/",
    "api-extractor": "npx api-extractor run --local",
    "clean": "rimraf typings src temp",
    "generate": "npm run generate-icons && tsc -p tsconfig.json && npm run api-extractor && npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ehsanbigzad/react-native-iconsax.git"
  },
  "keywords": [
    "icon",
    "react",
    "react-native-iconsax",
    "react-native",
    "native",
    "iconsax",
    "icons",
    "rn"
  ],
  "author": "Ehsan Bigzad",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ehsanbigzad/react-native-iconsax/issues"
  },
  "homepage": "https://github.com/ehsanbigzad/react-native-iconsax#readme",
  "peerDependencies": {
    "react-native": ">=0.46",
    "react-native-svg": ">=5.3"
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "@microsoft/api-extractor": "^7.16.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4"
  }
}
